class Person {
  var name string = "Gabriel"
  var age int = 19
  var height double = 110.1
  var isMale bool = true

  Person(name string, age int, height double, isMale bool) {
    name = name
    age = age
    height = height
    isMale = isMale
  }

  func prettyPrint() {
    print(name + age + height + isMale)
  }

}

const name string = "Gabriel Igliozzi"
const age int = 19
const height double  = 5.7
const isMale bool = false

const gabriel Person = Person(name, age, height / 1, !isMale)

if gabriel.age >= 20 {
  print(gabriel.name + " is old enough to enter"
} else {
  while(gabriel.age < 20) {
    print("not yet come back later")
  }
}
