class Person {

	//Const attributes
	const name string = "Gabriel"
	const lastName string = "Igliozzi"
	
	//var attributes
	var age int = 19
	var isAllowed bool = true

	func augmentAgeBy(amt int) void {
		age = age + amt
	}

	func getFullName() string {
		return name + " " lastName
	}

}

